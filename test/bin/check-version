#!/usr/bin/env node

'use strict';

var fs   = require('fs');
var path = require('path');

var matches   = /^(v(\d+))\.\d+/.exec(process.version);
var version   = (parseInt(matches[2], 10) > 0) ? matches[1] : matches[0];
var supported = fs.readdirSync(path.join(__dirname, '../../lib'));

process.exit(supported.indexOf(version) === -1 ? 1 : 0);
